generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Repairer {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  name      String
  password  String
  shopName  String?
  phone     String?
  city      String?
  createdAt DateTime @default(now())
}
